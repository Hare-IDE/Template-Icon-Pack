{
  "defaults": {
    "name": [{
      "light": "url/to/light.svg",
      "dark": "url/to/dark.svg"
    }]
  },
  "contextValues" : {
    "folder": {
      "default": [{
        "light": "url/to/light.svg",
        "dark": "url/to/dark.svg",
        "when": "state == collapsed"
      },{
        "light": "url/to/light.svg",
        "dark": "url/to/dark.svg",
        "when": "state == expanded"
      }],
      "regex": {
        "css": [{
          "light": "url/to/light.svg",
          "dark": "url/to/dark.svg",
          "when": "state == collapsed"
        },{
          "light": "url/to/light.svg",
          "dark": "url/to/dark.svg",
          "when": "state == expanded"
        }]
      }
    },
    "file": {
      "default": [{
        "light": "url/to/light.svg",
        "dark": "url/to/dark.svg"
      }],
      "regex": {
        "*.tsx": [{
          "light": "url/to/light.svg",
          "dark": "url/to/dark.svg"
        }]
      }
    }
  }
}